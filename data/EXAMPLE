curl -XGET 'localhost:9200/documents/document/_percolate' -d '{
    "doc" : {
        "text" : "A new bonsai tree in the office"
    }
}'

curl -XPUT 'localhost:9200/documents/.percolator/1' -d '{
    "query" : {
        "terms" : {
            "text" : [ "bonsai", "tree" ],
            "minimum-should-match" : "1"
        }
    }
}'

